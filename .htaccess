#ico e js 1 mÃªs de cache
<FilesMatch "\.(?i:ico|js)$">

  <IfModule mod_headers.c>
    Header set Cache-Control "max-age=2628000, public, must-revalidate"
  </IfModule>

</FilesMatch>

RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteBase /zenit/
RewriteRule ^(.*)$ index.php?/$1 [L]

